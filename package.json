{
  "name": "mylife-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --config vite.config.production.ts",
    "build:analyze": "node scripts/build-analyze.js",
    "build:stats": "npm run build && npm run analyze:bundle",
    "analyze:bundle": "npx serve dist -s -l 3001 & open http://localhost:3001/stats.html",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 20",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@tanstack/react-router": "^1.131.26",
    "@tanstack/react-router-devtools": "^1.131.26",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.540.0",
    "react": "19",
    "react-dom": "19",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tanstack/router-plugin": "^1.131.26",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "19",
    "@types/react-dom": "19",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  }
}
